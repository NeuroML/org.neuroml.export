        <Constant name="temp" dimension="none" value="21"/>
        <!--Note: this parameter (q10) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="q10" dimension="none" value="2.3"/>

        <Dynamics>
            <DerivedVariable name="V" dimension="none" value="v / VOLT_SCALE"/>
            <DerivedVariable name="Vshift" dimension="none" value="vshift"/>
            <DerivedVariable name="V05a" dimension="none" value="v05a"/>
            <DerivedVariable name="V05b" dimension="none" value="v05b"/>
            <DerivedVariable name="r" exposure="r" dimension="per_time" value="(a0*(V-a1)/(1-exp(-(V-a1)/a2))) / TIME_SCALE"/>
        </Dynamics>

