            <DerivedVariable name="cVth1" dimension="none" value="cvth1"/>
            <DerivedVariable name="cVth2" dimension="none" value="cvth2"/>
            <DerivedVariable name="t" exposure="t" dimension="time" value="((1e3) * ( ctm + 1. / ( exp(-((V+5) + cVtm1)/cktm1) + exp(-((V + 5)+cVtm2)/cktm2)) )) * TIME_SCALE"/>
