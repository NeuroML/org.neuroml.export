<?xml version="1.0" encoding="ISO-8859-1"?>
<neuroml xmlns="http://www.neuroml.org/schema/neuroml2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.neuroml.org/schema/neuroml2 https://raw.github.com/NeuroML/NeuroML2/development/Schemas/NeuroML2/NeuroML_v2beta4.xsd" id="iA">

    <notes>ChannelML file containing a single fast K+ Channel description from a cortical layer V pyramidal neuron, described in Keren, N., Peled, N., and Korngreen, A. (2005). Constraining compartmental models using multiple voltage recordings and genetic algorithms. Journal of neurophysiology, 94(6), 3730&#8211;42.</notes>

    <ionChannel id="iA" conductance="10pS" type="ionChannelHH" species="k">

        <notes/>
                
        <annotation>
            <rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
                <rdf:Description rdf:about="iA">
                    
                    <bqmodel:isDescribedBy xmlns:bqmodel="http://biomodels.net/model-qualifiers/">
                        <rdf:Bag>
                            <rdf:li>Keren, N., Peled, N., and Korngreen, A. (2005). Constraining compartmental models using multiple voltage recordings and genetic algorithms. Journal of neurophysiology, 94(6), 3730&#8211;42</rdf:li>
                            <rdf:li rdf:resource="http://www.ncbi.nlm.nih.gov/pubmed/16093338"/>
                        </rdf:Bag>
                    </bqmodel:isDescribedBy>

                </rdf:Description>
            </rdf:RDF>
        </annotation>

        <gate id="n" type="gateHHtauInf" instances="4">
            <q10Settings type="q10ExpTemp" q10Factor="2.3" experimentalTemp="21 degC"/>
            <timeCourse type="iA_n_tau_tau"/>
            <steadyState type="iA_n_inf_inf"/>
        </gate>

        <gate id="l" type="gateHHtauInf" instances="1">
            <q10Settings type="q10ExpTemp" q10Factor="2.3" experimentalTemp="21 degC"/>
            <timeCourse type="iA_l_tau_tau"/>
            <steadyState type="iA_l_inf_inf"/>
        </gate>
                            
    </ionChannel>

    <ComponentType name="iA_n_tau_tau" extends="baseVoltageDepTime">
        <Constant name="TIME_SCALE" dimension="time" value="1 ms"/>
        <Constant name="VOLT_SCALE" dimension="voltage" value="1 mV"/>
        <!--Note: this parameter (v05n) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="v05n" dimension="none" value="-47"/>
        <!--Note: this parameter (v05l) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="v05l" dimension="none" value="-66"/>
        <!--Note: this parameter (kn) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="kn" dimension="none" value="29"/>
        <!--Note: this parameter (kl) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="kl" dimension="none" value="-10"/>
        <!--Note: this parameter (qq) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="qq" dimension="none" value="5"/>
        <!--Note: this parameter (tq) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="tq" dimension="none" value="-55"/>
        <!--Note: this parameter (temp) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="temp" dimension="none" value="21"/>
        <!--Note: this parameter (q10) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="q10" dimension="none" value="2.3"/>
        <!--Note: this parameter (vmin) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="vmin" dimension="none" value="-120"/>
        <!--Note: this parameter (vmax) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="vmax" dimension="none" value="100"/>

        <Dynamics>
            <DerivedVariable name="V" dimension="none" value="v / VOLT_SCALE"/>
            <DerivedVariable name="V05n" dimension="none" value="v05n"/>
            <DerivedVariable name="V05l" dimension="none" value="v05l"/>
            <DerivedVariable name="Vmin" dimension="none" value="vmin"/>
            <DerivedVariable name="Vmax" dimension="none" value="vmax"/>
            <DerivedVariable name="t" exposure="t" dimension="time" value="(0.34+0.92*exp(-((V+71)/59)^2)) * TIME_SCALE"/>
        </Dynamics>

    </ComponentType>

    <ComponentType name="iA_n_inf_inf" extends="baseVoltageDepVariable">
        <Constant name="TIME_SCALE" dimension="time" value="1 ms"/>
        <Constant name="VOLT_SCALE" dimension="voltage" value="1 mV"/>
        <!--Note: this parameter (v05n) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="v05n" dimension="none" value="-47"/>
        <!--Note: this parameter (v05l) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="v05l" dimension="none" value="-66"/>
        <!--Note: this parameter (kn) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="kn" dimension="none" value="29"/>
        <!--Note: this parameter (kl) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="kl" dimension="none" value="-10"/>
        <!--Note: this parameter (qq) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="qq" dimension="none" value="5"/>
        <!--Note: this parameter (tq) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="tq" dimension="none" value="-55"/>
        <!--Note: this parameter (temp) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="temp" dimension="none" value="21"/>
        <!--Note: this parameter (q10) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="q10" dimension="none" value="2.3"/>
        <!--Note: this parameter (vmin) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="vmin" dimension="none" value="-120"/>
        <!--Note: this parameter (vmax) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="vmax" dimension="none" value="100"/>

        <Dynamics>
            <DerivedVariable name="V" dimension="none" value="v / VOLT_SCALE"/>
            <DerivedVariable name="V05n" dimension="none" value="v05n"/>
            <DerivedVariable name="V05l" dimension="none" value="v05l"/>
            <DerivedVariable name="Vmin" dimension="none" value="vmin"/>
            <DerivedVariable name="Vmax" dimension="none" value="vmax"/>
            <DerivedVariable name="x" exposure="x" dimension="none" value="1/(1 + exp(-(V-V05n)/kn))"/>
        </Dynamics>

    </ComponentType>

    <ComponentType name="iA_l_tau_tau" extends="baseVoltageDepTime">
        <Constant name="TIME_SCALE" dimension="time" value="1 ms"/>
        <Constant name="VOLT_SCALE" dimension="voltage" value="1 mV"/>
        <!--Note: this parameter (v05n) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="v05n" dimension="none" value="-47"/>
        <!--Note: this parameter (v05l) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="v05l" dimension="none" value="-66"/>
        <!--Note: this parameter (kn) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="kn" dimension="none" value="29"/>
        <!--Note: this parameter (kl) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="kl" dimension="none" value="-10"/>
        <!--Note: this parameter (qq) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="qq" dimension="none" value="5"/>
        <!--Note: this parameter (tq) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="tq" dimension="none" value="-55"/>
        <!--Note: this parameter (temp) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="temp" dimension="none" value="21"/>
        <!--Note: this parameter (q10) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="q10" dimension="none" value="2.3"/>
        <!--Note: this parameter (vmin) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="vmin" dimension="none" value="-120"/>
        <!--Note: this parameter (vmax) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="vmax" dimension="none" value="100"/>

        <Dynamics>
            <DerivedVariable name="V" dimension="none" value="v / VOLT_SCALE"/>
            <DerivedVariable name="V05n" dimension="none" value="v05n"/>
            <DerivedVariable name="V05l" dimension="none" value="v05l"/>
            <DerivedVariable name="Vmin" dimension="none" value="vmin"/>
            <DerivedVariable name="Vmax" dimension="none" value="vmax"/>
            <DerivedVariable name="t" exposure="t" dimension="time" value="(8+49*exp(-((V+73)/23)^2)) * TIME_SCALE"/>
        </Dynamics>

    </ComponentType>

    <ComponentType name="iA_l_inf_inf" extends="baseVoltageDepVariable">
        <Constant name="TIME_SCALE" dimension="time" value="1 ms"/>
        <Constant name="VOLT_SCALE" dimension="voltage" value="1 mV"/>
        <!--Note: this parameter (v05n) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="v05n" dimension="none" value="-47"/>
        <!--Note: this parameter (v05l) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="v05l" dimension="none" value="-66"/>
        <!--Note: this parameter (kn) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="kn" dimension="none" value="29"/>
        <!--Note: this parameter (kl) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="kl" dimension="none" value="-10"/>
        <!--Note: this parameter (qq) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="qq" dimension="none" value="5"/>
        <!--Note: this parameter (tq) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="tq" dimension="none" value="-55"/>
        <!--Note: this parameter (temp) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="temp" dimension="none" value="21"/>
        <!--Note: this parameter (q10) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="q10" dimension="none" value="2.3"/>
        <!--Note: this parameter (vmin) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="vmin" dimension="none" value="-120"/>
        <!--Note: this parameter (vmax) should ideally be defined only once within the ionChannel! Work in progress...-->
        <Constant name="vmax" dimension="none" value="100"/>

        <Dynamics>
            <DerivedVariable name="V" dimension="none" value="v / VOLT_SCALE"/>
            <DerivedVariable name="V05n" dimension="none" value="v05n"/>
            <DerivedVariable name="V05l" dimension="none" value="v05l"/>
            <DerivedVariable name="Vmin" dimension="none" value="vmin"/>
            <DerivedVariable name="Vmax" dimension="none" value="vmax"/>
            <DerivedVariable name="x" exposure="x" dimension="none" value="1/(1 + exp(-(V-V05l)/kl))"/>
        </Dynamics>

    </ComponentType>

</neuroml>
